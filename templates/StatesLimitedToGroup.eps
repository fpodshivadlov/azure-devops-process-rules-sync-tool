{
    "customizationType": "custom",
    "name": "<%= $RuleName %>",
    "conditions": [
        {
            "conditionType": "whenCurrentUserIsNotMemberOfGroup",
            "field": null,
            "value": "<%= $GroupId %>"
        }
    ],
    "actions": [
<% $States | Each { -%>
        {
            "actionType": "disallowValue",
            "targetField": "System.State",
            "value": "<%= $_ %>"
        }<% } -Join ",`n" %>
    ],
    "isDisabled": <%= Get-OrElse $Disabled "false" %>
}
